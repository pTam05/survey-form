<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="assignment.three.StudentBean" table="student">
		<id name="studentID"></id>

		<property name="firstName" type="java.lang.String"></property>
		<property name="lastName" type="java.lang.String"></property>
		<property name="address" type="java.lang.String"></property>
		<property name="zip" type="java.lang.Integer">
			<column name="zipcode" />
		</property>
		<property name="city" type="java.lang.String"></property>
		<property name="state" type="java.lang.String"></property>
		<property name="tel" type="java.lang.Long">
			<column name="phoneNum" />
		</property>
		<property name="email" type="java.lang.String"></property>
		<property name="url" type="java.lang.String"></property>
		<property name="dateOfSurvey" type="java.lang.String">
			<column name="form_date" />
		</property>
		<property name="info" type="java.lang.String">
			<column name="interestUniversity" />
		</property>
		<property name="interest" type="java.lang.String">
			<column name="likeCampus_db" />
		</property>
		<property name="recommendation" type="java.lang.String">
			<column name="recommend" />
		</property>
		<property name="comments" type="java.lang.String"></property>
		<property name="raffle" type="java.lang.String"></property>
		<property name="gradMonth" type="java.lang.String"></property>
		<property name="gradYear" type="java.lang.String"></property>

	</class>

</hibernate-mapping>